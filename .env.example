NODE_ENV=development
# MongoDB connection string
MONGO_URI=mongodb://localhost:27017/note-taker

# Backend port (optional, defaults to 5001)
PORT=5001

# Upstash (if using Upstash Redis for rate limiting)
UPSTASH_REDIS_REST_URL=https://<your-upstash-rest-url>
UPSTASH_REDIS_REST_TOKEN=<your-upstash-rest-token>

# CORS origin used by the frontend during development (no trailing slash)
CORS_ORIGIN=http://localhost:5173
